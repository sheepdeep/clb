{{>header}}
<body>
<div class="preloader"></div>
<!-- header -->
<header class="header">
    <div class="header__content">
        <!-- header logo -->
        <a href="/" class="header__logo">
            <img class="logo" src="{{ settings.logo }}" alt="{{ settings.nameSite }}"/>
        </a>
        <!-- end header logo -->
        <!-- header menu btn -->
        <button class="header__btn" type="button">
            <span></span>
            <span></span>
            <span></span>
        </button>
        <!-- end header menu btn -->
    </div>
</header>
<!-- end header -->
<!-- sidebar -->
{{>sidebar}}
<!-- main content -->
<main class="main">
    <div class="container-fluid">
        <div class="row">
            {{>game}}
            <div id="bnk-infos" class="row">
                <div class="col-12">
                    <div class="dashbox lst-bank-recharges">
                        <div class="dashbox__title">
                            <h3 style="margin:0 auto;">
                                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor"
                                     class="bi bi-bank2" viewBox="0 0 16 16">
                                    <path d="M8.277.084a.5.5 0 0 0-.554 0l-7.5 5A.5.5 0 0 0 .5 6h1.875v7H1.5a.5.5 0 0 0 0 1h13a.5.5 0 1 0 0-1h-.875V6H15.5a.5.5 0 0 0 .277-.916l-7.5-5zM12.375 6v7h-1.25V6h1.25zm-2.5 0v7h-1.25V6h1.25zm-2.5 0v7h-1.25V6h1.25zm-2.5 0v7h-1.25V6h1.25zM8 4a1 1 0 1 1 0-2 1 1 0 0 1 0 2zM.5 15a.5.5 0 0 0 0 1h15a.5.5 0 1 0 0-1H.5z"></path>
                                </svg>
                                DS BANK NẠP KBCOIN
                            </h3>
                        </div>
                        <div class="dashbox__table-wrap bi-wrap pb-0" data-scrollbar="true" tabindex="-1"
                             style="overflow: hidden; outline: none;">
                            <div class="scroll-content">
                                <table class="dashbox__table">
                                    <thead>
                                    <tr>
                                        <th>NGÂN HÀNG</th>
                                        <th>TÀI KHOẢN</th>
                                        <th>TÊN CTK</th>
                                        <th>NẠP MIN</th>
                                        <th>NẠP MAX</th>
                                    </tr>
                                    </thead>
                                    <tbody>
                                    {{#each banks}}
                                        <tr>
                                            <td>
                                                <div class="dashbox__table-text">
                                                    <span>MBBank</span>
                                                </div>
                                            </td>
                                            <td>
                                                <div class="dashbox__table-text">
                                                    <span>{{formatBank this.accountNumber}}</span>
                                                    <span>&nbsp;<a href="javascript:void(0);"
                                                                   data-id="{{ this.accountNumber }}"
                                                                   data-msg="Đã copy:  {{ this.accountNumber }}"
                                                                   class="mncp copy-bt">&nbsp;</a></span>
                                                </div>
                                            </td>
                                            <td>
                                                <span>{{ this.name }}</span>
                                            </td>
                                            <td>
                                                <div class="dashbox__table-text">{{ numberFormat this.betMin }}</div>
                                            </td>
                                            <td>
                                                <div class="dashbox__table-text">KHÔNG GIỚI HẠN</div>
                                            </td>
                                        </tr>
                                    {{/each}}
                                    </tbody>
                                </table>
                                <div class="subpage-wrapper">
                                    <p style="font-size:12px;">
                                        <span class="mr-5">CÚ PHÁP NẠP TIỀN</span><span class="arrow-right"></span><code
                                            class="pd5"><span
                                            id="kbnt-cpy">{{profile.username}} {{settings.paymentComment}}</span></code>
                                        <a href="javascript:void(0);"
                                           data-id="{{profile.username}} {{settings.paymentComment}}"
                                           data-msg="Đã copy : {{profile.username}} {{settings.paymentComment}}"
                                           class="mncp copy-bt">&nbsp;</a>
                                    </p>
                                    <p>
                                        <i class="small-notes text-center">
                                            LƯU Ý: ĐỂ <span class="code-num">NẠP TIỀN</span> HÃY SỬ DỤNG <span
                                                class="code-num">DANH SÁCH BANK NÀY</span>.<br>
                                            TIỀN NẠP VÀO SẼ ĐƯỢC SỬ DỤNG ĐỂ CHƠI <a href="/xxtg">TÀI XỈU</a>
                                        </i>
                                    </p>
                                </div>
                            </div>
                            <div class="scrollbar-track scrollbar-track-x show" style="display: none;">
                                <div class="scrollbar-thumb scrollbar-thumb-x"
                                     style="width: 685px; transform: translate3d(0px, 0px, 0px);"></div>
                            </div>
                            <div class="scrollbar-track scrollbar-track-y show" style="display: none;">
                                <div class="scrollbar-thumb scrollbar-thumb-y"
                                     style="height: 597px; transform: translate3d(0px, 0px, 0px);"></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="col-12">
                <div class="dashbox">
                    <div class="dashbox__title">
                        <h3 style="margin:0 auto;">
                            <svg xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24"
                                 stroke-width="2" stroke="currentColor" fill="none" stroke-linecap="round"
                                 stroke-linejoin="round">
                                <path stroke="none" d="M0 0h24v24H0z" fill="none"></path>
                                <path d="M3 21l18 0"></path>
                                <path d="M3 10l18 0"></path>
                                <path d="M5 6l7 -3l7 3"></path>
                                <path d="M4 10l0 11"></path>
                                <path d="M20 10l0 11"></path>
                                <path d="M8 14l0 3"></path>
                                <path d="M12 14l0 3"></path>
                                <path d="M16 14l0 3"></path>
                            </svg>
                            SBCOIN LÀ GÌ?
                        </h3>
                    </div>
                    <div class="subpage-wrapper">
                        <div class="text-center mb-3">
                            <p>
                                <span>KB COIN: </span> <span class="code-num"><b id="sbbal-txt">{{ numberFormat
                                    profile.balance }}</b></span> VNĐ
                                <span id="reload-kbbal" class="">
                            <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor"
                                 class="bi bi-arrow-clockwise" viewBox="0 0 16 16">
                                <path fill-rule="evenodd"
                                      d="M8 3a5 5 0 1 0 4.546 2.914.5.5 0 0 1 .908-.417A6 6 0 1 1 8 2z"></path>
                                <path d="M8 4.466V.534a.25.25 0 0 1 .41-.192l2.36 1.966c.12.1.12.284 0 .384L8.41 4.658A.25.25 0 0 1 8 4.466"></path>
                            </svg>
                        </span>
                                <br>
                                TỔNG CHƠI: <span class="code-num">{{ numberFormat totalPlay }}</span> ĐÃ RÚT: <span
                                    class="code-num">{{ numberFormat totalRutTien }}</span>
                            </p>
                            <div class="dashbox__title pt-0 pb-0">
                            </div>
                            <div style="max-width:380px;margin:0 auto;padding: 25px 0 10px 0;">
                                <p>
                                    <input autocomplete="off" class="sign__input format-num" id="wamount"
                                           placeholder="SỐ TIỀN MUỐN RÚT" type="text">
                                </p>
                                <p>
                                    <button class="sign__btn" type="button" id="btn-withdraw">
                                        <span>RÚT TIỀN VỀ BANK LK</span></button>
                                </p>
                            </div>
                            <div class="dashbox__title pt-0 pb-0">
                            </div>
                            <div style="max-width:380px;margin:0 auto;padding: 25px 0 10px 0;">
                                <p>
                                    <input autocomplete="off" class="sign__input" id="tnickname"
                                           placeholder="NICK-NAME NGƯỜI NHẬN" type="text">
                                </p>
                                <p>
                                    <button class="sign__btn" type="button" id="btn-transfer1"><span>CHUYỂN TIỀN CHO NGƯỜI KHÁC</span>
                                    </button>
                                </p>
                            </div>
                            <div class="dashbox__title pt-0 pb-0 mb-4">
                            </div>
                            <p>
                                <span class="code-num">SB-COIN</span> Là đơn vị tiền tệ được sử dụng để chơi một số game
                                khác ngoài chẵn lẻ bank trên <span class="code-num">KUBANK</span> <br>
                                Tỷ lệ quy đổi khi nạp là <span class="code-num">1 VNĐ = 1 SB-COIN</span> <br>
                                Hiện tại bạn có thể sử dụng <span class="code-num">SB-COIN</span> để chơi <a
                                    href="/xxtg">TÀI XỈU & CHẴN LẺ</a> <br>
                                Để chuyển tiền cho người chơi khác, tổng chơi <span
                                    class="code-num">TÀI XỈU & CHẴN LẺ</span> trong ngày của bạn cần đạt ít nhất <span
                                    class="code-num">1,000,000</span> vnđ
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
<div class="modal bounceIn animated" id="modal-notes" tabindex="-1" aria-labelledby="modal-view"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal__content modal__content--view">
                <div class="comments__autor">
                    <img class="comments__avatar" src="{{ settings.favicon }}" alt="">
                    <span class="comments__name">SUPBANK Thông Báo</span>
                </div>
                <p class="comments__text"></p>
                <div class="modal__btns">
                    <button class="modal__btn modal__btn--dismiss" type="button" data-bs-dismiss="modal"
                            aria-label="Close"><span>Đóng</span></button>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="modal bounceIn animated" id="modal-win" tabindex="-1" aria-labelledby="modal-view"
     aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered">
        <div class="modal-content">
            <div class="modal__content modal__content--view">
                <div class="comments__autor">
                    <img class="comments__avatar" src="{{ settings.favicon }}" alt="">
                    <span class="comments__name">SUPBANK Thông Báo</span>
                </div>
                <p class="comments__text"></p>
                <div class="modal__btns">
                    <button class="modal__btn modal__btn--dismiss" type="button" data-bs-dismiss="modal"
                            aria-label="Close"><span>Đóng</span></button>
                </div>
            </div>
        </div>
    </div>
</div>
<script>
    $(".nav-item button").click(function () {
        window.location.href = "/?g=" + $(this).attr("data-bs-target").slice(1)
    })
    document.getElementById("wamount").addEventListener("input", function() {
        let input = this.value;

        // Loại bỏ tất cả các ký tự không phải là số
        input = input.replace(/\D/g, '');

        // Format lại số
        this.value = formatNumber(input);
    });
</script>
{{>footer}}
{{{ settings.footerScript }}}
