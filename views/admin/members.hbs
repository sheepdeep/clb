{{>head}}
{{>nav}}
<main class="main-content">
    <div class="contents">
        <div class="crm mb-25">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="breadcrumb-main">
                            <h4 class="text-capitalize breadcrumb-title">{{ title }}</h4>
                        </div>
                    </div>
                    <div class="col-lg-12 mb-20">
                        <div class="card">
                            <div class="card-header">
                                <h6 class="card-title">
                                    <a href="{{ originalUrl.pathname }}" class="text-dark">
                                        <img src="../themes/admin/images/members.png" alt="" width="35">
                                        Danh Sách Thành Viên
                                        <img src="../themes/admin/images/members.png" class="d-md-none" width="35">
                                    </a>
                                </h6>
                            </div>
                            <div class="card-body">
                                <div class="userDatatable adv-table-table global-shadow border-light-0 adv-table">
                                    <div id="filter-form-container"
                                        class="footable-filtering-external footable-filtering-right">
                                        <form>
                                            <div class="form-group footable-filtering-search mb-3 hand">
                                                <div class="input-group">
                                                    <input type="search" class="form-control" name="search"
                                                        placeholder="Tìm kiếm..." value="{{ search }}">
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </div>
                                <div class="table-responsive mb-3">
                                    <table
                                        class="table table-vcenter text-nowrap table-bordered text-center table-mb sort-table">
                                        <thead class="badge-secondary text-white">
                                            <tr>
                                                <th>
                                                    <span data-bs-submit="members" data-bs-action="add">
                                                        <i class="fa fa-plus-circle" aria-hidden="true"></i>
                                                    </span>
                                                </th>
                                                <th>Họ Và Tên</th>
                                                <th>Tên Đăng Nhập</th>
                                                <th>Số Dư</th>
                                                <th>VIP </th>
                                                <th>Thông Tin Bank</th>
                                                <th>Telegram</th>
                                                <th>TOKEN</th>
                                                <th>Doanh Thu Ngày</th>
                                                <th>Doanh Thu Tháng</th>
                                                <th>Doanh Thu </th>
                                                <th>IP</th>
                                                <th>Đăng Nhập Cuối {{{ tableSort originalUrl.search 'lastOnline' _sort }}}</th>
                                            </tr>
                                        </thead>
                                        <tbody data-table="members">
                                            {{#each threads}}
                                            <tr class="edit-one" data-id="{{ this._id }}">
                                                <td>
                                                    <div class="table-actions hand">
                                                        <span class="badges badge-info editUser"
                                                            data-id="{{ this._id }}" data-bs-toggle="tooltip"
                                                            data-bs-placement="top"
                                                            data-bs-original-title="Chỉnh sửa quyền thành viên"><i
                                                                class="fas fa-pen"></i></span>
                                                        <span class="badges badge-danger" data-bs-submit="members"
                                                            data-bs-action="remove" data-id="{{ this._id }}"><i
                                                                class="fa fa-trash" aria-hidden="true"></i></span>
                                                    </div>
                                                </td>
                                                <td data-bs-name="name" data-bs-hasEdit="true">
                                                    <span data-bs-table="html" data-bs-value="{{ this.name }}">
                                                        <span data-bs-html="true">{{ this.name }}</span>
                                                    </span>
                                                    <span data-bs-table="edit" data-bs-value="{{ this.name }}">
                                                        <input type="text" class="form-control ih-medium ip-light mb-2"
                                                            data-bs-edit="true">
                                                    </span>
                                                </td>
                                                <td><a href="{{../adminPath}}/history?&search={{ this.username }}">{{ this.username }}</a></td>
                                                <td>{{ numberFormat this.balance }}</td>
                                                <td data-bs-name="level" data-bs-hasEdit="true">
                                                    <span data-bs-table="html" data-bs-value="{{ this.level }}">
                                                        <span data-bs-html="true">
                                                            {{#ifCond this.level '==' '1'}}
                                                            <span class="badges badge-success">Quản Trị Viên</span>
                                                            {{else}}
                                                            <span class="badges badge-warning">Cộng Tác Viên</span>
                                                            {{/ifCond}}
                                                        </span>
                                                    </span>
                                                    <span data-bs-table="edit" data-bs-value="{{ this.level }}">
                                                        <select class="form-control ih-medium ip-light mb-2"
                                                            data-bs-edit="true">
                                                            <option value="1">Quản Trị Viên</option>
                                                            <option value="2">Cộng Tác Viên</option>
                                                        </select>
                                                    </span>
                                                </td>
                                                <td data-bs-name="bankInfo.guard" data-bs-hasEdit="true">
                                                    <code>{{ this.bankInfo.accountNumber }}</code> - <code>{{ this.bankInfo.accountName }}</code> - <code>{{ bankName this.bankInfo.bankCode }}</code>
                                                    <br>
                                                    <span data-bs-table="html" data-bs-value="{{ this.bankInfo.guard }}">
                                                        <span data-bs-html="true">
                                                            {{#ifCond this.bankInfo.guard '==' '1'}}
                                                                <span class="badges badge-success">Khóa</span>
                                                            {{else}}
                                                                <span class="badges badge-danger">Mở</span>
                                                            {{/ifCond}}
                                                        </span>
                                                    </span>
                                                    <span data-bs-table="edit" data-bs-value="{{ this.bankInfo.guard }}">
                                                        <select class="form-control ih-medium ip-light mb-2" data-bs-edit="true">
                                                            <option value="true">Khóa</option>
                                                            <option value="false">Mở</option>
                                                        </select>
                                                    </span>
                                                </td>
                                                <td data-bs-name="telegram.status" data-bs-hasEdit="true">
                                                    <code>{{ this.telegram.chatId }}</code></code>
                                                    <br>
                                                    <span data-bs-table="html" data-bs-value="{{ this.telegram.status }}">
                                                        <span data-bs-html="true">
                                                            {{#ifCond this.telegram.status '==' 'active'}}
                                                                <span class="badges badge-success">Đã liên kết</span>
                                                            {{else}}
                                                                <span class="badges badge-danger">Chưa liên kết</span>
                                                            {{/ifCond}}
                                                        </span>
                                                    </span>
                                                    <span data-bs-table="edit" data-bs-value="{{ this.bankInfo.guard }}">
                                                        <select class="form-control ih-medium ip-light mb-2" data-bs-edit="true">
                                                            <option value="true">Khóa</option>
                                                            <option value="false">Mở</option>
                                                        </select>
                                                    </span>
                                                </td>
                                                <td><span class="badges badge-secondary">{{ this.token }}</span></td>
                                                <td><code><span class="text-success">{{ numberFormat this.historyDay.receipt }}</span></code> <br/> <code><span class="text-danger">{{ numberFormat this.historyDay.minus }}</span></code> <br /> <code><span class="text-warning">{{ numberFormat this.historyDay.earning }}</span></code></td>
                                                <td><code><span class="text-success">{{ numberFormat this.historyMonth.receipt }}</span></code> <br /> <code><span class="text-danger">{{ numberFormat this.historyMonth.minus }}</span></code> <br /> <code><span class="text-warning">{{ numberFormat this.historyMonth.earning }}</span></code></td>
                                                <td><code><span class="text-success">{{ numberFormat this.historyAll.receipt }}</span></code> <br /> <code><span class="text-danger">{{ numberFormat this.historyAll.minus }}</span></code> <br /> <code><span class="text-warning">{{ numberFormat this.historyAll.earning }}</span></code></td>
                                                <td>{{ this.ip }}</td>
                                                <td>{{ formatDate this.lastOnline 'HH:mm:ss DD-MM-YYYY'}}</td>
                                            </tr>
                                            {{else}}
                                            <tr>
                                                <td colspan="12">
                                                    <div class="dm-empty text-center">
                                                        <div class="dm-empty__image">
                                                            <img src="../themes/admin/images/1.png" alt="Rỗng">
                                                        </div>
                                                        <div class="dm-empty__text">
                                                            <p class="">Không có dữ liệu !</p>
                                                        </div>
                                                    </div>
                                                </td>
                                            </tr>
                                            {{/each}}
                                        </tbody>
                                    </table>
                                </div>
                                <div class="d-flex justify-content-end">
                                    <nav class="dm-page">
                                        <ul class="dm-pagination d-flex">
                                            <li class="dm-pagination__item">
                                                {{#paginate pagination type="previous"}}
                                                <a href="{{ baseURL }}?{{#if query }}{{ query }}&page={{n}}{{else}}page={{n}}{{/if}}"
                                                    class="dm-pagination__link pagination-control {{#if disabled}}
                                                    disabled{{/if}}">
                                                    <span class="la la-angle-left"></span>
                                                    </a>
                                                {{/paginate}}
                                                {{#paginate pagination type="middle" limit=pagination.limit}}
                                                <a href="{{ baseURL }}?{{#if query }}{{ query }}&page={{n}}{{else}}page={{n}}{{/if}}"
                                                    class="dm-pagination__link {{#if active}}active{{/if}}"><span
                                                        class="page-number">{{n}}</span>
                                                    </a>
                                                {{/paginate}}
                                                {{#paginate pagination type="next"}}
                                                <a href="{{ baseURL }}?{{#if query }}{{ query }}&page={{n}}{{else}}page={{n}}{{/if}}"
                                                    class="dm-pagination__link pagination-control {{#if disabled}}
                                                    disabled{{/if}}">
                                                    <span class="la la-angle-right"></span>
                                                    </a>
                                                {{/paginate}}
                                            </li>
                                            <li class="dm-pagination__item">
                                                <div class="paging-option">
                                                    <select name="perPage" class="page-selection"
                                                            data-query="{{ checkQuery originalUrl.search 'perPage' }}">
                                                        {{#select perPage }}
                                                            <option value="10">10/page</option>
                                                            <option value="20">20/page</option>
                                                            <option value="40">40/page</option>
                                                            <option value="60">60/page</option>
                                                            <option value="100">100/page</option>
                                                            <option value="200">200/page</option>
                                                            <option value="500">500/page</option>
                                                        {{/select}}
                                                    </select>
                                                </div>
                                            </li>
                                        </ul>
                                    </nav>
                                </div>

                            </div>
                        </div>
                    </div>
                    <div class="col-lg-12 mb-20">
                        <div class="card">
                            <div class="card-header">
                                <h6 class="card-title">
                                    <a href="{{ originalUrl.pathname }}" class="text-dark">
                                        <img src="../themes/admin/images/action_member.png" alt="" width="35">
                                        Thao Tác Thành Viên
                                        <img src="../themes/admin/images/action_member.png" class="d-md-none"
                                            width="35">
                                    </a>
                                </h6>
                            </div>
                            <div class="card-body">
                                <form action="members/action" method="post" class="form-axios" reload="true">
                                    <div class="form-group">
                                        <label for="" class="il-gray fs-14 fw-500 mb-10">Tên Đăng Nhập</label>
                                        <select name="username" class="form-control ih-medium ip-light select2">
                                            {{#each users}}
                                            <option value="{{ this.username }}">{{ this.username }}</option>
                                            {{/each}}
                                        </select>
                                    </div>
                                    <div class="form-group">
                                        <label for="" class="il-gray fs-14 fw-500 mb-10">Nội Dung</label>
                                        <input type="text" name="message" class="form-control ih-medium ip-light"
                                            placeholder="Nhập nội dung cần thông báo đến thành viên ( có thể bỏ trống )">
                                    </div>
                                    <div class="form-group">
                                        <label for="" class="il-gray fs-14 fw-500 mb-10">URL</label>
                                        <input type="text" name="url" class="form-control ih-medium ip-light"
                                            placeholder="Nhập link truy cập tới ( có thể bỏ trống )">
                                    </div>
                                    <div class="form-group">
                                        <label for="" class="il-gray fs-14 fw-500 mb-10">Hành Động</label>
                                        <select name="action" class="form-control ih-medium-ip-light">
                                            <option value="">NO</option>
                                            <option value="reload">Tải Lại Trang</option>
                                            <option value="dismiss">Kích</option>
                                            <option value="checkInfo">Thông Tin</option>
                                        </select>
                                    </div>
                                    <div class="content-center">
                                        <button class="btn btn-primary"><i class="fa fa-plus-circle"
                                                aria-hidden="true"></i> Thực Hiện</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</main>
<!-- Modal Add Members -->
